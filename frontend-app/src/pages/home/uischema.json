{
    "type": "VerticalLayout",
    "elements": [
        {
            "type": "HorizontalLayout",
            "elements": [
                {
                    "type": "Control",
                    "scope": "#/properties/name"
                },
                {
                    "type": "Control",
                    "scope": "#/properties/personalData/properties/age"
                },
                {
                    "type": "Control",
                    "scope": "#/properties/birthDate"
                }
            ]
        },
        {
            "type": "Label",
            "text": "Additional Information"
        },
        {
            "type": "HorizontalLayout",
            "elements": [
                {
                    "type": "Control",
                    "scope": "#/properties/personalData/properties/height"
                },
                {
                    "type": "Control",
                    "scope": "#/properties/nationality"
                },
                {
                    "type": "Control",
                    "scope": "#/properties/occupation",
                    "suggestion": [
                        "Accountant",
                        "Engineer",
                        "Freelancer",
                        "Journalism",
                        "Physician",
                        "Student",
                        "Teacher",
                        "Other"
                    ]
                }
            ]
        },
        {
            "type": "GridLayout",
            "scope": "#",
            "options": {
                "style": {
                    "direction": "row",
                    "spacing": 1,
                    "justifyContent": "flex-end"
                }
            },
            "elements": [
                {
                    "type": "ActionButton",
                    "elements": [
                        {
                            "type": "Typography",
                            "label": "Back To Login"
                        }
                    ],
                    "options": {
                        "style": {
                            "variant": "contained"
                        },
                        "onclick": {
                            "route": {
                                "type": "ui",
                                "uri": "/app1"
                            }
                        }
                    }
                }
            ]
        }
    ]
}